@model Projecto.Models.Ticket

<div class="container">
  <h2>Delete Ticket</h2>
  <p>Are you sure you want to delete ticket "@Model.Title"</p>
  <form asp-controller="Tickets" asp-action="DeleteConfirmed">
    <input type="hidden" asp-for="Id">
    <button type="submit" class="btn btn-danger">Confirm</button>
    <a href="javascript:history.back()" class="btn btn-secondary">Back</a>
  </form>
</div>